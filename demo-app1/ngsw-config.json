{
  "index": "/index.html",
  "assetGroups": [
    {
      "name": "app1",
      "installMode": "prefetch",
      "updateMode": "prefetch",
      "resources": {
        "files": [
          "/favicon.ico",
          "/index.html",
          "/*.css",
          "/*.js"
        ]
      }
    }, {
      "name": "assets1",
      "installMode": "lazy",
      "updateMode": "lazy",
      "resources": {
        "files": [
          "/assets/**"
        ]
      }
    }
  ],
  "dataGroups": [
    {
      "name": "https://jsonplaceholder.typicode.com",
      "urls": ["https://jsonplaceholder.typicode.com/**"],
      "cacheConfig": {
        "maxSize": 3,
        "maxAge": "7d",
        "strategy": "freshness"
      }
    }
  ]
}
